<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>RecommandService</servlet-name>
  	<display-name>RecommandService</display-name>
  	<description></description>
  	<servlet-class>com.smhrd.controller.RecommandService</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>BoardDAO</servlet-name>
  	<display-name>BoardDAO</display-name>
  	<description></description>
  	<servlet-class>com.smhrd.model.BoardDAO</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>BoardVO</servlet-name>
  	<display-name>BoardVO</display-name>
  	<description></description>
  	<servlet-class>com.smhrd.model.BoardVO</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RecommandService</servlet-name>
  	<url-pattern>/RecommandService</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>BoardDAO</servlet-name>
  	<url-pattern>/BoardDAO</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>BoardVO</servlet-name>
  	<url-pattern>/BoardVO</url-pattern>
  </servlet-mapping>
</web-app>
